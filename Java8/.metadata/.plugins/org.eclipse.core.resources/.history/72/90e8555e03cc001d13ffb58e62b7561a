package com.sikku.pack5;

import java.util.function.Predicate;

public class Test {

	public static void main(String[] args) {
		Predicate<String> p = s-> s.startsWith("k");
		
		String [] str = {"kas","kllr","","rger","","rkgjrk"};
		
		for(String s: str) {
			if(p.test(s)){
				System.out.println(s);				
			}
		}
	}
}
